    "@nestjs/mapped-types": "^2.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.5",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^5.10.2",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^8.1.0",
    "mysql2": "^3.9.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.14.0",
    "rxjs": "^7.8.1"
